table(align="center" border="1" cellpadding="0" cellspacing="0" width="600" style={borderCollapse: 'collapse'})
    tr
        th='Host'
        th='Success'
        th='Failure'
    each record in records
        tr
            td= record.host
            td(style={color: 'green'})= record.success
            td(style={color: 'red'})= record.failure